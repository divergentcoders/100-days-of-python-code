height=int(input("enter your height for rollercoaster ride \n"))
if height>=120:
  age=int(input("enter your age \n"))
  if age<18:
    print("you are not eligible for ride due to age \n")
  elif age>=18 and age<=60:
    print("you are eligible for ride \n")
  else:
    print("you are not eligible for ride \n")
else:
  print("u are not eligible for ride \n")
